{
  "extends": ["next/core-web-vitals", "next", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "ignorePatterns": [
    "node_modules/*",
    ".next/*",
    ".out/*",
    "!.prettierrc.js",
    "!.eslintrc.js"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
